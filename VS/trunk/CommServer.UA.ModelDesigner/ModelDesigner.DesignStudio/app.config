      <setting name="TargetNamespace" serializeAs="String">
        <value>http://cas.eu/UA/CommServer/</value>
      </setting>
      <setting name="TargetXmlNamespace" serializeAs="String">
        <value>http://cas.eu/UA/CommServer/</value>
      </setting>
      <setting name="ShowSplashScreen" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="MaintenancePeriodWarning" serializeAs="String">
