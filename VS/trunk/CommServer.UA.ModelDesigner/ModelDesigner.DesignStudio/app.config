<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<configSections>
		<sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="CAS.UA.Model.Designer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
		</sectionGroup>
		<sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
			<section name="CAS.UA.Model.Designer.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
		</sectionGroup>
	</configSections>
	<startup>
	<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" /></startup>
	<applicationSettings>
		<CAS.UA.Model.Designer.Properties.Settings>
   <setting name="HelpDocumentationOpcUaEbookURL" serializeAs="String">
    <value>http://www.commsvr.com/UAModelDesigner/Index.aspx</value>
   </setting>
   <setting name="HelpDocumentationURLsuffix" serializeAs="String">
    <value>?topic=</value>
   </setting>
   <setting name="HelpDocumentationAllTopicsWebAddress" serializeAs="String">
    <value>http://www.commsvr.com/UAModelDesigner/allTopics.xml</value>
   </setting>
   <setting name="HelpDocumentationAllTopicsFileName" serializeAs="String">
    <value>allTopics.xml</value>
   </setting>
   <setting name="SampleInstallationDirectory" serializeAs="String">
    <value>UAAddressSpaceModelDesigner\Sample</value>
   </setting>
  </CAS.UA.Model.Designer.Properties.Settings>
	</applicationSettings>
	<userSettings>
		<CAS.UA.Model.Designer.Properties.Settings>
   <setting name="DefaultSolutionFileName" serializeAs="String">
    <value>UAModelDesignerSolution</value>
   </setting>
   <setting name="FontSize" serializeAs="String">
    <value>8</value>
   </setting>
   <setting name="CoupledNodesAreEnabled" serializeAs="String">
    <value>True</value>
   </setting>
   <setting name="HelpSynchronizationIsEnabled" serializeAs="String">
    <value>True</value>
   </setting>
   <setting name="XmlUATypesNamespace" serializeAs="String">
    <value>http://opcfoundation.org/UA/2008/02/Types.xsd</value>
   </setting>
   <setting name="XmlUATypesPrefix" serializeAs="String">
    <value>ua</value>
   </setting>
   <setting name="Build_ProjectCompilationString" serializeAs="String">
    <value>-d2 "{0}"  -cg "{1}" -o "{2}" -console</value>
   </setting>
   <setting name="ProjectCompilationExecutable" serializeAs="String">
    <value>exe\Cas.Opc.Ua.ModelCompiler.exe</value>
   </setting>
   <setting name="TargetNamespaceXmlPrefix" serializeAs="String">
    <value>cas</value>
   </setting>
   <setting name="XmlSchemaInstanceNamespace" serializeAs="String">
    <value>http://www.w3.org/2001/XMLSchema-instance</value>
   </setting>
   <setting name="XmlSchemaInstancePrefix" serializeAs="String">
    <value>xsi</value>
   </setting>
   <setting name="XmlSchemaNamespace" serializeAs="String">
    <value>http://www.w3.org/2001/XMLSchema</value>
   </setting>
   <setting name="XmlSchemaPrefix" serializeAs="String">
    <value>xsd</value>
   </setting>
   <setting name="XmlOPCTypesNamespace" serializeAs="String">
    <value>http://opcfoundation.org/UA/2008/02/Types.xsd</value>
   </setting>
   <setting name="XmlOPCTypesPrefix" serializeAs="String">
    <value>uax</value>
   </setting>
   <setting name="TargetNamespace" serializeAs="String">
    <value>http://cas.eu/UA/CommServer/</value>
   </setting>
   <setting name="TargetXmlNamespace" serializeAs="String">
    <value>http://cas.eu/UA/CommServer/</value>
   </setting>
   <setting name="ShowSplashScreen" serializeAs="String">
    <value>True</value>
   </setting>
   <setting name="MaintenancePeriodWarning" serializeAs="String">
    <value>True</value>
   </setting>
   <setting name="OPCViewerAssemblyName" serializeAs="String">
    <value>CAS.CommServer.ModelDesigner.OPCViewer.exe</value>
   </setting>
  </CAS.UA.Model.Designer.Properties.Settings>
	</userSettings>
	<system.diagnostics>
		<sources>
			<source name="CAS.UA.Common.TraceEvent" switchName="CAS.UA.Model.Designer.TraceEvent.SW" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<add name="LogFile" />
					<add name="DebugOutput" />
					<add name="CAS.Model.Designer.OutputWindow.Listener" />
					<remove name="Default" />
				</listeners>
			</source>
      <source name="CAS.UA.Server.ServerConfiguration" switchName="CAS.UA.Server.ServerConfiguration.Switch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="DebugOutput" />
          <add name="CAS.Model.Designer.OutputWindow.Listener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.UA.Model.Designer.Configuration.TraceEvent" switchName="CAS.UA.Model.Designer.TraceEvent.SW" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<add name="LogFile" />
					<add name="DebugOutput" />
					<add name="CAS.Model.Designer.OutputWindow.Listener" />
					<remove name="Default" />
				</listeners>
			</source>
			<source name="CAS.UA.Model.Designer.TraceEvent" switchName="CAS.UA.Model.Designer.TraceEvent.SW" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<add name="LogFile" />
					<add name="DebugOutput" />
					<add name="CAS.Model.Designer.OutputWindow.Listener" />
					<remove name="Default" />
				</listeners>
			</source>
			<source name="CAS.CodeProtect" switchName="CAS_CodeProtect_Switch" switchType="System.Diagnostics.SourceSwitch">
				<listeners>
					<!--
          <add name="console"
            type="System.Diagnostics.ConsoleTraceListener">
            <filter type="System.Diagnostics.EventTypeFilter" initializeData="Warning"/>
          </add>
          -->
					<add name="LogFile" />
					<!--<remove name="Default" />-->
				</listeners>
			</source>
		</sources>
		<switches>
			<!--ms-help://MS.VSCC.v80/MS.MSDN.v80/MS.NETDEVFX.v20.en/cpref6/html/T_System_Diagnostics_SourceLevels.htm-->
			<!-- SourceLevels Enumeration -->
			<add name="CAS.UA.Model.Designer.TraceEvent.SW" value="All" />
			<!-- value:  Required String attribute. Sets the verbosity of the output. 
                  Legitimate values are Critical, Error, Verbose, Warning, and Information.
      -->
			<!-- maxdatasize:  Optional Int32 attribute. Sets the maximum number of bytes of network data included in each line trace. 
                         The default value is 1024.
      -->
			<!-- tracemode Optional String attribute. Set to includehex to show protocol traces in hexadecimal and text format. 
      Set to protocolonly to show only text. The default value is includehex.
      -->
      <add name="CAS.UA.Server.ServerConfiguration.Switch" value="All" />
      <add name="CAS_CodeProtect_Switch" value="All" />
		</switches>
		<sharedListeners>
			<add name="DebugOutput" type="System.Diagnostics.ConsoleTraceListener" initializeData="false">
				<!-- Allows all events through. -->
				<filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
				<!-- Allows the Stop, Start, Suspend, Transfer, and Resume events through. -->
				<!-- <filter type="System.Diagnostics.EventTypeFilter" initializeData="ActivityTracing" /> -->
				<!-- Allows only Critical events through.  -->
				<!-- <filter type="System.Diagnostics.EventTypeFilter" initializeData="Critical" /> -->
				<!-- Allows Critical and Error events through.  -->
				<!-- <filter type="System.Diagnostics.EventTypeFilter" initializeData="Error" /> -->
				<!-- Allows Critical, Error, Warning, and Information events through. -->
				<!-- <filter type="System.Diagnostics.EventTypeFilter" initializeData="Information" /> -->
				<!-- Does not allow any events through. -->
				<!-- <filter type="System.Diagnostics.EventTypeFilter" initializeData="Off" /> -->
				<!-- Allows Critical, Error, Warning, Information, and Verbose events through.  -->
				<!-- <filter type="System.Diagnostics.EventTypeFilter" initializeData="Verbose" /> -->
				<!-- Allows Critical, Error, and Warning events through. -->
				<!-- <filter type="System.Diagnostics.EventTypeFilter" initializeData="Warning" /> -->
			</add>
			<add name="LogFile" type="CAS.Lib.RTLib.Diagnostics.AdvancedDelimitedListTraceListener, CAS.RTLibComm" initializeData="|ApplicationDataPath|\log\CAS.OPC.UA.ASMD.log" traceOutputOptions="DateTime">
				<filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
			</add>
			<add name="CAS.Model.Designer.OutputWindow.Listener" type="CAS.Lib.ControlLibrary.TextBoxBaseTraceListener,CAS.ControlLibrary">
				<filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
			</add>
		</sharedListeners>
	</system.diagnostics>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="CAS.CommServer.DA.Server.Configuration" publicKeyToken="8832ff1a67ea61a3" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.1.2.24782" newVersion="4.1.2.24782" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
