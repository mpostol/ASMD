      <setting name="TargetNamespaceXmlPrefix" serializeAs="String">
        <value>cas</value>
      </setting>
      <setting name="XmlSchemaInstanceNamespace" serializeAs="String">
        <value>http://www.w3.org/2001/XMLSchema-instance</value>
