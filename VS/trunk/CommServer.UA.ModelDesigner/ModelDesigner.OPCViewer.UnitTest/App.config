<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <sources>

      <!--CAS-->
      <source name="TracesFromEventLogMonitor" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.CodeProtect.TraceSource" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.RealTime" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.Windows.Forms" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.CommServer.CommonBus" switchValue="All"  switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>

      <!--CAS.CommServer.OPCClassicSDK.Core-->
      <source name="Opc.Tracer" switchValue="All"  switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>

      <!--CAS.CommServer.OPCClassicSDK.COMWrapper-->
      <source name="OpcCom.Tracer" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>

      <!--CAS.CommServer.DA.Client-->
      <source name="CAS.CommServer.DA.Client.Operations" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>

      <!--CAS.CommServer.DA.Viewer-->
      <source name="CAS.CommServer.DA.Viewer.FormControls" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
      <source name="CAS.CommServer.ModelDesigner.OPCViewer" switchValue="All" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="LogFile" />
          <add name="OPCViewer.Output.TraceListener" />
          <remove name="Default" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="LogFile" type="CAS.Lib.CodeProtect.EnvironmentAccess.AdvancedDelimitedListTraceListener,CAS.CodeProtect" initializeData="|ApplicationDataPath|\CAS.CommServer.DA.Viewer.log" traceOutputOptions="DateTime">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
      </add>
      <add name="OPCViewer.Output.TraceListener" type="CAS.Lib.ControlLibrary.TextBoxBaseTraceListener,CAS.Windows.Forms">
        <filter type="System.Diagnostics.EventTypeFilter" initializeData="All" />
      </add>
    </sharedListeners>
    <trace autoflush="true" />
  </system.diagnostics>

</configuration>